<% if current_user.admin? %>
  <%= form_with(model: @room, remote: true, class:'d-flex',
    data: { controller: "reset-form", action: "turbo:submit-end->reset-form#reset"}) do |f| %>
    <%= f.text_field :name, class: 'form-control', autocomplete: 'off', placeholder: "Admins Only" %>
    <%= f.submit data: {turbo: "true"}, class: 'btn btn-primary add_room' %>
  <% end %>
<% end %>